{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost;*.localhost",
  "HttpsRedirection": {
    "RedirectStatusCode": 307,
    "HttpsPort": 5001
  },
  "Hsts": {
    "Preload": true,
    "IncludeSubDomains": true,
    "MaxAge": "365.00:00:00"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=reactivites.db"
  },
  "Database": {
    "ClearDataOnStartup": false,
    "ReseedOnStartup": false
  }
}
